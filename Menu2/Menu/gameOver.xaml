<Window x:Class="Menu2.Menu.gameOver"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Menu2.Menu"
        mc:Ignorable="d"
        Title="gameOver" Height="500" Width="500" WindowStyle="None"  WindowStartupLocation="CenterScreen">
    <Viewbox Stretch="Fill">
        <Canvas Width="500" Height="500">
            <Button x:Name="btRestart" Content="Начать заново" HorizontalAlignment="Left" VerticalAlignment="Top" Canvas.Left="168" Canvas.Top="154" Height="72" Width="164" Click="btResume_Click"/>
            <Button x:Name="btExit" Content="Выйти из игры" HorizontalAlignment="Left" VerticalAlignment="Top" Click="btExit_Click" Height="49" Canvas.Left="162" Canvas.Top="345" Width="176"/>
        </Canvas>
    </Viewbox>
</Window>
