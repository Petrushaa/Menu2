<Page x:Class="Menu2.GamePlay"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Menu2"
        mc:Ignorable="d"
        Title="GamePlay" Height="800" Width="1554">
    <Viewbox Stretch="Fill">
        <Canvas x:Name="GameScreen" Focusable="True" KeyUp="KeyboardUp" KeyDown="KeyBoardDown" Height="800" Width="1554">
            <Canvas.Background>
                <ImageBrush ImageSource="/Spawn/spawn.jpg"/>
            </Canvas.Background>
            <Image x:Name="Character" Tag="User" Width="50" Height="50" HorizontalAlignment="Center" VerticalAlignment="Top" Canvas.Left="752" Canvas.Top="375" Stretch="Fill" Source="/Spawn/characterLeft.png"/>
            <Image Height="100" Tag="Collide" Canvas.Left="1086" Canvas.Top="375" Width="100" Source="/Spawn/wall.png" Stretch="Fill"/>
        </Canvas>
    </Viewbox>
</Page>
